<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:buttons="clr-namespace:Kalendarzyk.Views.CustomControls.Buttons"
             x:Class="Kalendarzyk.Views.CustomControls.ChangableIconCC"
             x:Name="this">

    <buttons:ClickableFontButtonCC         
        IconText="{Binding IconFontText, Source={x:Reference this}}"
        SubmitCommand="{Binding IsSelectedCommand, Source={x:Reference this}}" 
        IconTextColor="{Binding IconTextColor, Source={x:Reference this}}" 
        Margin="10" FontSize="44"/>
</ContentView>
